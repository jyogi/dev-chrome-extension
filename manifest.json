{
  "manifest_version": 2,
  "name": "Cloudinsight Dev",
  "version": "0.3.0",
  "description": "Developer Plugin for Cloudinsight",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "lib/background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "*://*.oneapm.com/*",
    "*://*.cloudinsight.cc/*",
    "https://*.wilddogio.com/*",
    "http://www.google-analytics.com/",
    "alarms",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "tabs",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.oneapm.com/*",
        "*://*.cloudinsight.cc/*"
      ],
      "js": [
        "lib/content.js"
      ],
      "runat": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Cloudinsight Dev",
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "author": "wyvernnot",
  "homepage_url": "https://github.com/cloudinsight/dev-chrome-extension"
}
